{
  "name": "contacts-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.4.1",
    "react-transition-group": "^4.3.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^1.3.5"
  },
  "scripts": {
    "build:tailwind": "postcss src/tailwind.styles.css -o src/tailwind.generated.css",
    "watch:tailwind": "postcss -w src/tailwind.styles.css -o src/tailwind.generated.css",
    "start": "run-p watch:tailwind start:react",
    "start:react": "react-scripts start",
    "prebuild": "npm run build:tailwind",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5001",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "autoprefixer": "^9.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
